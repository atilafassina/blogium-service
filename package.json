{
  "name": "blogium-service",
  "version": "2.0.0",
  "description": "medium xml to json",
  "main": "index.js",
  "author": "@atilafassina",
  "license": "MIT",
  "scripts": {
    "test": "xo && nyc ava",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
